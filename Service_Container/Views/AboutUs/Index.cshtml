@model AboutUsIndexVM
    <!-- Breadcrumb Section Begin -->
    <div class="breadcrumb-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb-text">
                        <h2>About Us</h2>
                        <div class="bt-option">
                            <a asp-controller="Home">Home</a>
                            <span>About Us</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Breadcrumb Section End -->
    <!-- About Us Page Section Begin -->
    <section class="aboutus-page-section spad">
        <div class="container">
            <div class="about-page-text">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="ap-title">
                            <h2>@Model.AboutUsSections.FirstOrDefault().Title</h2>
                            <p>
                                @Model.AboutUsSections.FirstOrDefault().SmallDescription
                            </p>
                        </div>
                    </div>
                    <div class="col-lg-5 offset-lg-1">
                        <ul class="ap-services">
                            @for (int i = 0; i < @Model.AboutUsSections.FirstOrDefault().AboutUsPreferncesSections.Count; i++)
                            {
                                <li><i class="icon_check"></i> @Model.AboutUsSections.FirstOrDefault().AboutUsPreferncesSections.ElementAt(i).Prefernce</li>
                            }
                        </ul>
                    </div>
                </div>
            </div>
            <div class="about-page-services">
                <div class="row">
                    @for (int i = 0; i < @Model.AboutUsSections.FirstOrDefault().AboutUsImageSections.Count; i++)
                    {
                        <div class="col-md-4">
                            <div class="ap-service-item set-bg" data-setbg="img/about/about-p1.jpg">
                                <img src="~/img/about/@Model.AboutUsSections.FirstOrDefault().AboutUsImageSections.ElementAt(i).Image" />
                                <div class="api-text">
                                    <h3>@Model.AboutUsSections.FirstOrDefault().AboutUsImageSections.ElementAt(i).ImageTitle</h3>
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </section>
    <!-- About Text Begin-->
    <section class="new-about">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb-text">
                        <h2>@Model.AboutTextSection.Title</h2>
                    </div>
                    <p class="f-para">
                        @Model.AboutTextSection.AboutCompanyActivity
                    </p>
                </div>
            </div>
        </div>
    </section>

    <!-- About Text End-->
    <!-- Gallery Section Begin -->
    <section class="gallery-section spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title">
                        <span>@Model.GallerySections.FirstOrDefault().Title</span>
                        <h2>@Model.GallerySections.FirstOrDefault().SubTitle</h2>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-6">
                    <div class="gallery-item set-bg" data-setbg="img/gallery/gallery-1.jpg" style="background-image: url(" img /gallery/gallery-1.jpg");">
                        <img src="~/img/gallery/@Model.GallerySections.FirstOrDefault().GalleryImageSections.ElementAt(0).Image" />
                        <div class="gi-text">
                            <h3>@Model.GallerySections.FirstOrDefault().GalleryImageSections.ElementAt(0).ImageTitle</h3>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-6">
                            <div class="gallery-item set-bg" data-setbg="img/gallery/gallery-3.jpg" style="background-image: url(" img /gallery/gallery-3.jpg");">
                                <img src="~/img/gallery/@Model.GallerySections.FirstOrDefault().GalleryImageSections.ElementAt(1).Image" />
                                <div class="gi-text">
                                    <h3>@Model.GallerySections.FirstOrDefault().GalleryImageSections.ElementAt(1).ImageTitle</h3>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="gallery-item set-bg" data-setbg="img/gallery/gallery-4.jpg" style="background-image: url(" img /gallery/gallery-4.jpg");">
                                <img src="~/img/gallery/@Model.GallerySections.FirstOrDefault().GalleryImageSections.ElementAt(2).Image" />
                                <div class="gi-text">
                                    <h3>@Model.GallerySections.FirstOrDefault().GalleryImageSections.ElementAt(2).ImageTitle</h3>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="gallery-item large-item set-bg" data-setbg="img/gallery/gallery-2.jpg" style="background-image: url(" img /gallery/gallery-2.jpg");">
                        <img src="~/img/gallery/@Model.GallerySections.FirstOrDefault().GalleryImageSections.ElementAt(3).Image" />
                        <div class="gi-text">
                            <h3>@Model.GallerySections.FirstOrDefault().GalleryImageSections.ElementAt(3).ImageTitle</h3>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Gallery Section End -->
 